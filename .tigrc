bind main <Ctrl-P> !git push
bind tree <Ctrl-r> ?sh -c 'echo %(file) >> .gitignore'

color cursor         default color99 bold
color search-result  default color93
color author         green   default
color status         green   default
color line-number    red     default
color title-focus    default color99 bold
color title-blur     default magenta
color diff-header    yellow  default
color diff-index     blue    default
color diff-chunk     magenta default
color "Reported-by:" green   default
color tree.date      cyan    default bold
